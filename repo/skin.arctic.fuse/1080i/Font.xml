<?xml version="1.0" encoding="UTF-8"?>
<fonts>

    <fontset id="Default" unicode="false">
        <include content="Font_Default">
            <param name="font_black">AljazeeraFull.ttf</param>
            <param name="font_bold">AljazeeraFull.ttf</param>
            <param name="font_regular">AljazeeraFull.ttf</param>
            <param name="font_light">AljazeeraFull.ttf</param>
            <param name="aspect_system">1.1</param>
        </include>
    </fontset>	

    <fontset id="Amazon" unicode="true">
        <include content="Font_Default">
            <param name="font_black">AmazonFullRegular.ttf</param>
